{
  "name": "dadaberry-website",
  "version": "1.0.0",
  "description": "DadaBerry website built with NestJS for Netlify",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build && npm run build:netlify",
    "build:netlify": "tsc netlify/functions/serverless.ts --outDir netlify/functions --esModuleInterop --module commonjs --target ES2020 --moduleResolution node --resolveJsonModule --skipLibCheck --experimentalDecorators --emitDecoratorMetadata",
    "start": "nest start",
    "dev": "nest start --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/platform-express": "^10.3.0",
    "@netlify/functions": "^2.4.0",
    "express": "^4.18.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "serverless-http": "^3.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.1.1",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

